<script>
	$(document).ready(function(){
		
		$.ajax({
			url : "card", // server url
			type : 'POST',
			data : {
				'username': username,
				'password': password
			}, 
			datatype : 'json',
			beforeSend : function() {
			},
			success : function(data) {
				if(data == "false")
					location.href = "index.html";
				else
					location.href="card.html"
				console.log(data);
			},
			complete : function() {
				signupSuccess();
			},
			error : function(xhr, status, error) {
				alert(xhr.responseText); // error occur 
			}
		});
		
	});
</script>
